let arr = array(5, null);

// > [null, null, null, null, null]
print("~\n", arr);

arr[0] <- 1;
arr[1] <- 2;
arr[2] <- 3;

// > [1, 2, 3, null, null]
print("~\n", arr);

// > [0]: 1
// > [1]: 2
// > [2]: 3
// > [3]: null
// > [4]: null
print("[0]: ~\n", arr[0]);
print("[1]: ~\n", arr[1]);
print("[2]: ~\n", arr[2]);
print("[3]: ~\n", arr[4]);
print("[4]: ~\n", arr[4]);